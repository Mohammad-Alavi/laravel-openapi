{
    "name": "vyuldashev/laravel-openapi",
    "description": "Generate OpenAPI Specification for Laravel Applications",
    "keywords": [
        "laravel",
        "openapi",
        "api",
        "documentation",
        "docs",
        "rest",
        "swagger"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Yuldashev",
            "email": "misterio92@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "doctrine/dbal": "^2.6|^3.0",
        "goldspecdigital/oooas": "^2.7.1",
        "laravel/framework": "5.8.*|^6.0|^7.0|^8.0|^9.0|^10.0",
        "friendsofphp/php-cs-fixer": "^3.8.0",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^5.8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3|^6.0|^7.0",
        "phpunit/phpunit": "^9.5.13"
    },
    "autoload": {
        "psr-4": {
            "Vyuldashev\\LaravelOpenApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vyuldashev\\LaravelOpenApi\\Tests\\": "tests/",
            "Examples\\Petstore\\": "examples/petstore/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-protocols": [
            "https"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vyuldashev\\LaravelOpenApi\\OpenApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
