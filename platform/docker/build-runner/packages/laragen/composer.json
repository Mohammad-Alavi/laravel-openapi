{
    "name": "mohammad-alavi/laragen",
    "description": "1-click OpenAPI generation from Laravel repositories",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "mohammad-alavi/laravel-openapi": "*",
        "mohammad-alavi/laravel-rules-to-schema": "*",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "knuckleswtf/scribe": "^5.2",
        "myclabs/deep-copy": "^1.12",
        "scalar/laravel": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "MohammadAlavi\\Laragen\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MohammadAlavi\\Laragen\\Providers\\LaragenServiceProvider"
            ]
        }
    }
}
