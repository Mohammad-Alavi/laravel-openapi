{
    "name": "mohammad-alavi/laravel-rules-to-schema",
    "description": "Convert Laravel validation rules to JSON Schema",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "MohammadAlavi\\LaravelRulesToSchema\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MohammadAlavi\\LaravelRulesToSchema\\Providers\\RulesToSchemaServiceProvider"
            ]
        }
    }
}
